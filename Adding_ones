#include<stdio.h>
#include<stdlib.h>
#define size 1010
int main()
{
	int i,j,N,K,T,x,u,v,y,h,l;
	int A[size];
	scanf("%d",&T);
	for(v=0;v<T;v++)
	{
	 scanf("%d %d",&N,&K);
	 for(h=1;h<=N;h++)
	 {
	     A[h]=0;
	 }
	 for(u=0;u<K;u++)
	 {
	    scanf("%d ",&j);
	    for(i=1;i<=N;i++)
	    {
	        if(i>=j)
	        {
	            A[i]=A[i]+1;
	        }
	    }
	    
	   
	 }
	 for(l=1;l<=N;l++)
	    {
	        printf("%d ",A[l]);
	    }
	 printf("\n");
	}
	
	return 0;
}
